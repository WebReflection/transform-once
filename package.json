{
  "name": "transform-once",
  "version": "0.0.0",
  "description": "A basic helper to simplify relations among values",
  "main": "./cjs/index.js",
  "scripts": {
    "build": "npm run cjs && npm run test",
    "cjs": "ascjs --no-default esm cjs",
    "test": "c8 node test/index.js",
    "coverage": "mkdir -p ./coverage; c8 report --reporter=text-lcov > ./coverage/lcov.info"
  },
  "keywords": [
    "weakly",
    "transformed",
    "references",
    "primitives"
  ],
  "author": "Andrea Giammarchi",
  "license": "ISC",
  "devDependencies": {
    "ascjs": "^5.0.1",
    "c8": "^7.11.0",
    "weak-value": "^0.2.3"
  },
  "module": "./esm/index.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "default": "./cjs/index.js"
    },
    "./package.json": "./package.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WebReflection/transform-once.git"
  },
  "bugs": {
    "url": "https://github.com/WebReflection/transform-once/issues"
  },
  "homepage": "https://github.com/WebReflection/transform-once#readme"
}